{
  "name": "ronda-penal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint \"{packages/**/{src,__tests__}/**/*.{ts,tsx,js,jsx},packages/*config*/**/*.{ts,tsx,js,jsx}}\" --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "server:dev": "yarn workspace @ronda-penal/server dev",
    "server:build": "yarn workspace @ronda-penal/server build",
    "server:start": "yarn workspace @ronda-penal/server start",
    "web-admin:dev": "yarn workspace @ronda-penal/web-admin dev",
    "web-admin:build": "yarn workspace @ronda-penal/web-admin build",
    "web-admin:preview": "yarn workspace @ronda-penal/web-admin preview",
    "app:start": "yarn workspace @ronda-penal/app start",
    "app:run-android": "yarn workspace @ronda-penal/app android",
    "app:run-ios": "yarn workspace @ronda-penal/app ios",
    "test": "jest --verbose"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "license": "MIT-0",
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@ronda-penal/eslint-config": "*",
    "@types/jest": "^29.5.1",
    "babel-jest": "^29.2.1",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "react-toastify": "^10.0.5"
  }
}
